---
import { Icon } from "astro-icon/components";

// data imports
import navData from "../data/navData";
import Link from "./Link.astro";
---

<nav aria-label="Primary">
  <div class="navbar">
    <a href="/" aria-label="Go home" class="'logomark">
      <Icon width="60" name="logomark" /></a
    >
    <ul>
      {
        navData.map((item) => {
          return (
            <Link
              
              text={item.name}
              href={item.path}
              style="secondary"
              isFilled={false}
              data-navLink
            />
          );
        })
      }
    </ul>
  </div>
</nav>


<script>
  import '../js/nav.js'
</script>